{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "127.0.0.1",
          "Port": 5000
        }
      ],
      "UpstreamPathTemplate": "/gateway/1/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://127.0.0.1:7700"
  }
}